{
  "name": "mobile",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "coverage": "jest --coverage --watchAll=false",
    "lint": "eslint ."
  },
  "dependencies": {
    "@react-native-community/async-storage": "^1.9.0",
    "@react-native-community/datetimepicker": "^2.4.0",
    "@react-native-community/masked-view": "^0.1.9",
    "@react-navigation/bottom-tabs": "^5.2.8",
    "@react-navigation/native": "^5.1.5",
    "@react-navigation/stack": "^5.2.10",
    "axios": "^0.21.1",
    "date-fns": "^2.13.0",
    "immer": "^6.0.3",
    "prop-types": "^15.7.2",
    "react": "16.11.0",
    "react-native": "0.62.2",
    "react-native-gesture-handler": "^1.6.1",
    "react-native-reanimated": "^1.8.0",
    "react-native-safe-area-context": "^0.7.3",
    "react-native-screens": "^2.4.0",
    "react-native-see-more-inline": "^1.1.0",
    "react-native-text-size": "^4.0.0-rc.1",
    "react-native-vector-icons": "^6.6.0",
    "react-redux": "^7.2.0",
    "reactotron-react-native": "^5.0.0",
    "reactotron-redux": "^3.1.3",
    "reactotron-redux-saga": "^4.2.3",
    "redux": "^4.0.5",
    "redux-saga": "^1.1.3",
    "styled-components": "^5.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/runtime": "^7.9.2",
    "@react-native-community/eslint-config": "^1.0.0",
    "@testing-library/jest-native": "^3.1.0",
    "@testing-library/react-native": "^5.0.3",
    "@types/jest": "^25.2.3",
    "axios-mock-adapter": "^1.18.1",
    "babel-jest": "^25.3.0",
    "babel-plugin-root-import": "^6.5.0",
    "eslint": "^6.8.0",
    "eslint-import-resolver-babel-plugin-root-import": "^1.1.1",
    "jest": "^25.3.0",
    "metro-react-native-babel-preset": "^0.59.0",
    "react-test-renderer": "16.11.0"
  },
  "jest": {
    "preset": "@testing-library/react-native",
    "setupFiles": [
      "./jest.setup.js"
    ],
    "setupFilesAfterEnv": [
      "@testing-library/jest-native/extend-expect"
    ],
    "testMatch": [
      "**/__tests__/**/*.test.js"
    ],
    "collectCoverageFrom": [
      "!__tests_/**/*.*",
      "!src/index.js",
      "!src/App.js",
      "!src/services/**/*.js",
      "!src/store/**/*.js",
      "!src/configs/**/*.js",
      "!src/context/**/*.js",
      "!src/routes/**/*.js",
      "!android/**/*.*",
      "!ios/**/*.*",
      "!src/components/SplashScreen/**.js",
      "!src/pages/SettingsUser/**.js"
    ],
    "coverageDirectory": "__tests__/coverage",
    "moduleNameMapper": {
      "^~/(.*)": "<rootDir>/src/$1"
    },
    "transformIgnorePatterns": [
      "<rootDir>/bower_components/",
      "<rootDir>/node_modules/@react-native-community/datetimepicker'"
    ]
  }
}
